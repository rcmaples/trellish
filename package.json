  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rcmaples/trellish/issues"
  },
  "homepage": "https://github.com/rcmaples/trellish#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "codecov": "^3.1.0",
    "dotenv": "^6.0.0",
    "expect": "^23.5.0",
    "faker": "^4.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.3.0",
    "lodash.isboolean": "^3.0.3",
    "lodash.pick": "^4.4.0",
