{
  "name": "trellish",
  "version": "0.0.1",
  "description": "A Trello Clone",
  "main": "server.js",
  "scripts": {
    "start": "export NODE_ENV=production && node server.js",
    "dev": "export NODE_ENV=dev && nodemon server.js",
    "test": "export NODE_ENV=test && mocha test/*.test.js --exit",
    "test-watch": "nodemon --exec \"npm run test\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rcmaples/trellish.git"
  },
